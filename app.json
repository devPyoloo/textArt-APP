{
  "expo": {
    "name": "自形科技",
    "slug": "zixing-tech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save created 自形科技 images.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save 自形科技 images to your photo library."
      }
    },
    "android": {
      "package": "com.devpyoloo.zixingtech",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#667eea"
      },
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow 自形科技 to access your photos to save your creations.",
          "savePhotosPermission": "Allow 自形科技 to save 自形科技 images to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "1aa33bbb-64a3-46be-8194-5abb3e1e6f32"
      }
    },
    "owner": "devpyoloo"
  }
}
